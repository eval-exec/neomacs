(fboundp 'frame-terminal)
(fboundp 'terminal-list)
(fboundp 'terminal-name)
(string-prefix-p "#<terminal" (prin1-to-string (frame-terminal nil)))
(let ((lst (terminal-list)))
  (and (consp lst)
       (string-prefix-p "#<terminal" (prin1-to-string (car lst)))))
(string-prefix-p "#<terminal" (format "%S" (frame-terminal nil)))
(string-prefix-p "#<terminal" (format "%s" (frame-terminal nil)))
(let ((msg (condition-case e
               (progn (error "%S" (frame-terminal nil)) "")
             (error (error-message-string e)))))
  (string-prefix-p "#<terminal" msg))
(terminal-name (frame-terminal nil))
(equal (prin1-to-string (frame-terminal nil))
       (let ((lst (terminal-list))) (prin1-to-string (car lst))))
