; NeoVM default policy: `#[...]` reader literals are vectors, not executable byte-code objects.

(byte-code-function-p (car (read-from-string "#[nil \"\\300\\207\" [0] 1]")))
(functionp (car (read-from-string "#[nil \"\\300\\207\" [0] 1]")))
(type-of (car (read-from-string "#[nil \"\\300\\207\" [0] 1]")))
(condition-case err
    (funcall (car (read-from-string "#[nil \"\\300\\207\" [42] 1]")))
  (error (car err)))
(condition-case err
    (apply (car (read-from-string "#[(x) \"\\10\\207\" [x] 1]")) '(7))
  (error (car err)))
