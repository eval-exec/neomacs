(condition-case err (read-coding-system "" 'utf-8) (error err))
(condition-case err (read-coding-system "" 1) (error err))
(condition-case err (read-coding-system nil) (error err))
(condition-case err (read-coding-system 1) (error err))
(condition-case err (read-coding-system "" nil nil) (error err))
(condition-case err (read-non-nil-coding-system "") (error err))
(condition-case err (read-non-nil-coding-system nil) (error err))
(condition-case err (read-non-nil-coding-system 1) (error err))
(condition-case err (read-non-nil-coding-system "" nil) (error err))
